<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".swipeupActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="156dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/slideinidicator" />

    <SeekBar
        android:id="@+id/seekBar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="269dp"
        android:layout_height="22dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="124dp"
        android:max="10"
        android:progress="3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/contributor"
        android:layout_width="121dp"
        android:layout_height="37dp"
        android:layout_marginTop="412dp"
        android:layout_marginEnd="220dp"
        android:text="@string/contributor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/radius"
        android:layout_width="121dp"
        android:layout_height="37dp"
        android:layout_marginTop="364dp"
        android:layout_marginEnd="220dp"
        android:text="@string/radius"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/areaNickname"
        android:layout_width="121dp"
        android:layout_height="37dp"
        android:layout_marginTop="172dp"
        android:layout_marginEnd="220dp"
        android:text="@string/area_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/rating"
        android:layout_width="121dp"
        android:layout_height="37dp"
        android:layout_marginTop="220dp"
        android:layout_marginEnd="220dp"
        android:text="@string/rating"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/longitude"
        android:layout_width="121dp"
        android:layout_height="37dp"
        android:layout_marginTop="268dp"
        android:layout_marginEnd="220dp"
        android:text="@string/longitude"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/latitude"
        android:layout_width="121dp"
        android:layout_height="37dp"
        android:layout_marginTop="316dp"
        android:layout_marginEnd="220dp"
        android:text="@string/latitude"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/contributorText"
        android:layout_width="121dp"
        android:layout_height="37dp"
        android:layout_marginTop="412dp"
        android:layout_marginEnd="72dp"
        android:background="@drawable/userinputtext"
        android:focusable="false"
        android:inputType="none"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/radiusText"
        android:layout_width="121dp"
        android:layout_height="37dp"
        android:layout_marginTop="364dp"
        android:layout_marginEnd="72dp"
        android:background="@drawable/userinputtext"
        android:focusable="false"
        android:inputType="none"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/ratingText"
        android:layout_width="121dp"
        android:layout_height="37dp"
        android:layout_marginTop="220dp"
        android:layout_marginEnd="72dp"
        android:background="@drawable/userinputtext"
        android:focusable="false"
        android:inputType="none"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/latitudeText"
        android:layout_width="121dp"
        android:layout_height="37dp"
        android:layout_marginTop="316dp"
        android:layout_marginEnd="72dp"
        android:background="@drawable/userinputtext"
        android:focusable="false"
        android:inputType="none"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/longitudeText"
        android:layout_width="121dp"
        android:layout_height="37dp"
        android:layout_marginTop="268dp"
        android:layout_marginEnd="72dp"
        android:background="@drawable/userinputtext"
        android:focusable="false"
        android:inputType="none"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/areaNicknameText"
        android:layout_width="121dp"
        android:layout_height="37dp"
        android:layout_marginTop="172dp"
        android:layout_marginEnd="72dp"
        android:background="@drawable/userinputtext"
        android:focusable="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/addLocationButton"
        android:layout_width="114dp"
        android:layout_height="46dp"
        android:layout_marginStart="216dp"
        android:layout_marginTop="528dp"
        android:text="Button"
        android:onClick="AddLocation"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>